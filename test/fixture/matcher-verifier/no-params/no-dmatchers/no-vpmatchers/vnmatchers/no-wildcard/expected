# matchers
00 variadic a: 7
01 variadic b: 7

# matching - order a
isMatch = true
00 variadic a: match
01 variadic b: match

# matching - order b
isMatch = true
00 variadic a: match
01 variadic b: match

# non-matching - empty
isMatch = false
00 variadic a: non-match (omitted)
01 variadic b: non-match (omitted)

# non-matching - missing named arg a
isMatch = false
00 variadic a: non-match (omitted)
01 variadic b: match

# non-matching - missing named arg b
isMatch = false
00 variadic a: match
01 variadic b: non-match (omitted)

# non-matching - all non-match
isMatch = false
00 variadic a: non-match
01 variadic b: non-match

# non-matching - named a non-match
isMatch = false
00 variadic a: non-match
01 variadic b: match

# non-matching - named b non-match
isMatch = false
00 variadic a: match
01 variadic b: non-match

# non-matching - extra positional arguments
isMatch = false
00 variadic 0: non-match (extra)
01 variadic 1: non-match (extra)
02 variadic a: match
03 variadic b: match

# non-matching - extra mixed arguments
isMatch = false
00 variadic 0: non-match (extra)
01 variadic 1: non-match (extra)
02 variadic a: match
03 variadic b: match
04 variadic y: non-match (extra)
05 variadic z: non-match (extra)

# non-matching - extra named arguments
isMatch = false
00 variadic a: match
01 variadic b: match
02 variadic y: non-match (extra)
03 variadic z: non-match (extra)

# non-matching - all non-match, extra arguments
isMatch = false
00 variadic 0: non-match (extra)
01 variadic 1: non-match (extra)
02 variadic a: non-match
03 variadic b: non-match
04 variadic y: non-match (extra)
05 variadic z: non-match (extra)
